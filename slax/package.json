{
  "name": "slax-skydoku",
  "version": "0.9.0",
  "description": "The skydoku slax application.",
  "main": "./index",
  "scripts": {
    "build": "slib build ."
  },
  "homepage": "skydoku.hudaokeji.com",
  "keywords": [
    "skydoku",
    "slax"
  ],
  "author": "Hudaokeji Co.,Ltd",
  "dependencies": {
    "skylark-slax-runtime": "git+https://github.com/skylark-slax/skylark-slax-runtime.git",
    "skylark-faiconpicker2": "git+https://github.com/skylark-integration/skylark-faiconpicker2.git",
    "skylark-jqueryui": "git+https://github.com/skylark-integration/skylark-jqueryui.git",
    "skylark-jqueryui-interact": "git+https://github.com/skylark-integration/skylark-jqueryui-interact.git",
    "skylark-jszip": "git+https://github.com/skylark-integration/skylark-jszip.git",
    "skylark-particles": "git+https://github.com/skylark-integration/skylark-particles.git",
    "skylark-widgets-coder": "git+https://github.com/skylark-widgets/skylark-widgets-coder.git",
    "skylark-widgets-textpad": "git+https://github.com/skylark-widgets/skylark-widgets-textpad.git",
    "skylark-widgets-wordpad": "git+https://github.com/skylark-widgets/skylark-widgets-wordpad.git"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "skylarkjs": {
    "directories": {
      "dependencies": "./node_modules"
    },
    "secondaries": {},
    "build": {
      "prepare": {
        "texttojs": [
          "html",
          "json",
          "css"
        ],
        "jsxtojs": false,
        "es6toamd": false,
        "cjstoamd": false
      },
      "bundles": {
        "all": true
      }
    }
  }
}
